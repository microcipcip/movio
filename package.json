{
  "homepage": "http://microcipcip.github.io/movio",
  "name": "movio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "polished": "^2.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-icons": "^3.2.2",
    "react-loader-spinner": "^2.3.0",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "jest-dom": "^3.0.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-testing-library": "^5.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
